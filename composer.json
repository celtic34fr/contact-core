{
  "name": "celtic34fr/contact-core",
  "description": "ðŸ“¦ Bolt extension Contact Core base functionalities.",
  "type": "bolt-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Gilbert ARMENGAUD",
      "email": "gilbert.armengaud@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "bolt/core": ">=5.1",
    "twig/twig": "^2.12 | ^3.0",
    "nucleos/dompdf-bundle": "^3.1",
    "teamtnt/tntsearch": "^3.1"
  },
  "require-dev": {
    "symplify/easy-coding-standard": "^8.3",
    "phpunit/phpunit": "^9.6",
    "symfony/browser-kit": "^6.3",
    "symfony/css-selector": "^6.3",
    "symfony/phpunit-bridge": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Celtic34fr\\ContactCore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "Celtic34fr\\ContactCore\\Tests\\": "tests/",
        "Bolt\\Tests\\": "vendor/bolt/common/tests/phpunit/unit/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "entrypoint": "Celtic34fr\\ContactCore\\Extension"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "drupol/composer-packages": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
